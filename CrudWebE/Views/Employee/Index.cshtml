@model List< CrudWebE.Models.Employeecs>
@{
}
<h1 style="color:peachpuff; text-align:center; padding:20px; margin:20px" >Employees</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">NAME</th>
      <th scope="col">Email</th>
      <th scope="col">Date OF Birth</th>
      <th scope="col">Salary</th>
      <th scope="col">Department</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
        @{
            foreach (var employee in Model )
            {
                 <tr>
                 <td>@employee.EmpId</td>
                 <td>@employee.Name</td>
                 <td>@employee.Email</td>
                 <td>@employee.DateofBirth.ToString("dd-mm-yyyy")</td>
                 <td>@employee.Salary</td>
                 <td>@employee.Department</td>
                 <td><a href = "Employee/view/@employee.EmpId">View</a></td>

      
    </tr>

                
            }

      }
   
  </tbody>
</table>